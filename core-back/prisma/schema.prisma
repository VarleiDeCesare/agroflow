generator client {
  provider = "prisma-client-js"
}
datasource db {
  provider = "postgresql"
  url      = "postgresql://admin:admin@localhost:5432/agrotehc?schema=public"
}

model User {
  id         String   @id @default(uuid())
  name       String?
  surname    String?
  email      String   @unique
  password   String?
  cpf        String?
  created_at DateTime @default(now())
  updated_at DateTime @updatedAt

  @@map("users")
}
